.container {
  display: flex;
  flex: 1;
  flex-direction: column;
  box-sizing: border-box;
}

.slide .swp {
  height: 400rpx;
}

.slide .nav-img {
  height: 400rpx;
  width: 100%;
}

.board {
  
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.board-item {
  flex-grow:1;
  text-align: center;
  cursor: pointer;
  border: 1rpx solid #eee;
  padding:40rpx;
}


